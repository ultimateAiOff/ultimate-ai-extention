{
  "manifest_version": 3,
  "name": "ultimate ai extention",
  "version": "0.1",
  "description": "extention obligatoire pour utiliser ultimate ai qui bloque tous les chatbot ia",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}   